{
  "name": "manyhands-site",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "css": "tailwindcss --input ./css/main.css --output ./dist/styles.css",
    "css:watch": "tailwindcss --input ./css/main.css --output ./dist/styles.css --watch",
    "sync": "browser-sync start --server --files ./dist/styles.css index.html",
    "build": "css",
    "dev": "npm-run-all --parallel css sync"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wearelighthouse/ManyHands-site.git"
  },
